<div class="bg-white w-screen top-0" style="height: calc(100vh - 70px)">
  <div class="overflow-scroll h-full p-10">
    <% @venues.each do |venue| %>
      <%= render 'venue', venue: venue %>
    <% end %>
    <div class="flex justify-end">
      <%= link_to 'Add Venue', new_venue_path(@venue), class:'flex-col text-white py-1 px-6 focus:outline-none hover:bg-blue-600 rounded-md text-lg bg-gradient-to-t from-blue-900 via-blue-600 to-blue-500' %>
    </div>
  </div>
</div>



<script>
  window.routeParamsFromRails = {
    user_id: "<%= current_user.id %>",
    user_email: "<%= current_user.email %>",
    user_token: "<%= current_user.authentication_token %>",
    vapid_public_key: "<%= ENV['VAPID_PUBLIC_KEY'] %>",
  };
</script>
